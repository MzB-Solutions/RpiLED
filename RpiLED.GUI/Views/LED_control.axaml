<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="150" d:DesignHeight="100"
             x:Class="RpiLED.Gui.Views.LED_control">
    <StackPanel HorizontalAlignment="Center" Background="#191919">
        <VirtualizingStackPanel HorizontalAlignment="Center">
            <TextBlock>Pin #</TextBlock>
            <CheckBox>State</CheckBox>
            <ToggleSwitch OffContent="Input" OnContent="Output"></ToggleSwitch>
        </VirtualizingStackPanel>
    </StackPanel>
</UserControl>
