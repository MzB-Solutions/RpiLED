<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RpiLed.Gui.Views.ShiftRegisterView">
  <DockPanel>
      <Button DockPanel.Dock="Bottom" Content="Test ShiftRegisterView" />
      <StackPanel>
          <TextBox Margin="4" Padding="4">SD_I Pin</TextBox>
          <TextBox Margin="4" Padding="4">SR_CLK Pin</TextBox>
          <TextBox Margin="4" Padding="4">SC_CLK Pin</TextBox>
      </StackPanel>
  </DockPanel>
</UserControl>
